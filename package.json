{
	"name": "french-typos",
	"version": "1.0.10",
	"description": "Automatic typographic apostrophe, French quotation marks and dialog mark",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"French",
		"typographic",
		"keyboard"
	],
	"author": "tcrouzet",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git://github.com/tcrouzet/obsidian-french-typos.git"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
